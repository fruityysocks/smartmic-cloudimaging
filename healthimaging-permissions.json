{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ReadOrganizedDICOM",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::target-healthimaging-test",
        "arn:aws:s3:::target-healthimaging-test/*"
      ]
    },
    {
      "Sid": "WriteImportResults",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": [
        "arn:aws:s3:::output-healthimaging-test"
      ]
    }
  ]
}