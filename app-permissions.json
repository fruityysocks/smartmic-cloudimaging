{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3Access",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::source-healthimaging-test",
        "arn:aws:s3:::source-healthimaging-test/*",
        "arn:aws:s3:::target-healthimaging-test",
        "arn:aws:s3:::target-healthimaging-test/*",
        "arn:aws:s3:::output-healthimaging-test",
        "arn:aws:s3:::output-healthimaging-test/*"
      ]
    },
    {
      "Sid": "HealthImagingAccess",
      "Effect": "Allow",
      "Action": [
        "medical-imaging:CreateDatastore",
        "medical-imaging:ListDatastores",
        "medical-imaging:GetDatastore",
        "medical-imaging:DeleteDatastore",
        "medical-imaging:StartDICOMImportJob",
        "medical-imaging:GetDICOMImportJob",
        "medical-imaging:ListDICOMImportJobs",
        "medical-imaging:SearchImageSets",
        "medical-imaging:GetImageSet",
        "medical-imaging:GetImageSetMetadata",
        "medical-imaging:GetImageFrame",
        "medical-imaging:DeleteImageSet"
      ],
      "Resource": "*"
    },
    {
      "Sid": "PassRoleToHealthImaging",
      "Effect": "Allow",
      "Action": "iam:PassRole",
      "Resource": "arn:aws:iam::207799300633:role/HealthImagingServiceRole",
      "Condition": {
        "StringEquals": {
          "iam:PassedToService": "medical-imaging.amazonaws.com"
        }
      }
    }
  ]
}